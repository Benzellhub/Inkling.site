{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-undef": "off",
    "prefer-arrow-callback": "off",
    "prefer-const": "off",
    "eqeqeq": "off",
    "no-param-reassign": "off",
    "no-use-before-define": "off"
  }
}
